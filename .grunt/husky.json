            "__component__$dropdown@husky": "husky_components/dropdown/main",
            "__component__$matrix@husky": "husky_components/matrix/main",
            "__component__$select@husky": "husky_components/select/main",
            "__component__$dependent-select@husky": "husky_components/dependent-select/main",
            "__component__$search@husky": "husky_components/search/main",
            "__component__$tabs@husky": "husky_components/tabs/main",
            "__component__$auto-complete@husky": "husky_components/auto-complete/main",
            "__component__$auto-complete-list@husky": "husky_components/auto-complete-list/main",
            "__component__$password-fields@husky": "husky_components/password-fields/main",
            "__component__$column-navigation@husky": "husky_components/column-navigation/main",

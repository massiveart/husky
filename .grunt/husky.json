{
    "namespace": "Husky",
    "requireJS": {
        "baseUrl": ".",
        "preserveLicenseComments": false,
        "paths": {
            "aura": "bower_components/aura/lib",
            "underscore": "bower_components/underscore/underscore",
            "eventemitter": "bower_components/eventemitter2/lib/eventemitter2",
            "backbone": "bower_components/backbone/backbone",
            "requirejs": "bower_components/requirejs/require",
            "text": "bower_components/requirejs-text/text",
            "jquery": "bower_components/jquery/jquery",
            "husky-validation": "bower_components/husky-validation/dist/validation",
            "globalize_lib": "bower_components/globalize/lib/globalize",
            "uri-template": "bower_components/massiveart-uritemplate/uritemplate",
            "husky": "lib/husky",

            "__component__$navigation@husky": "husky_components/navigation/main",
            "__component__$header@husky": "husky_components/header/main",
            "__component__$button@husky": "husky_components/button/main",
            "__component__$datagrid@husky": "husky_components/datagrid/main",
            "__component__$dialog@husky": "husky_components/dialog/main",
            "__component__$dropdown@husky": "husky_components/dropdown/main",
            "__component__$matrix@husky": "husky_components/matrix/main",
            "__component__$select@husky": "husky_components/select/main",
            "__component__$search@husky": "husky_components/search/main",
            "__component__$tabs@husky": "husky_components/tabs/main",
            "__component__$auto-complete@husky": "husky_components/auto-complete/main",
            "__component__$dropdown-multiple-select@husky": "husky_components/dropdown-multiple-select/main",
            "__component__$password-fields@husky": "husky_components/password-fields/main"
        },
        "shim": {
            "backbone": {
                "exports": "Backbone",
                "deps": ["underscore", "jquery"]
            },
            "underscore": {
                "exports": "_"
            }
        },
        "include": [
            "jquery",
            "requirejs",
            "underscore",
            "backbone",
            "text",
            "aura/ext/debug",
            "aura/ext/mediator",
            "aura/ext/components",
            "husky-validation",
            "globalize_lib",
            "uri-template",

            "husky",
            "__component__$navigation@husky",
            "__component__$header@husky",
            "__component__$button@husky",
            "__component__$datagrid@husky",
            "__component__$dialog@husky",
            "__component__$dropdown@husky",
            "__component__$matrix@husky",
            "__component__$select@husky",
            "__component__$search@husky",
            "__component__$tabs@husky",
            "__component__$auto-complete@husky",
            "__component__$dropdown-multiple-select@husky",
            "__component__$password-fields@husky"
        ]
    }
}

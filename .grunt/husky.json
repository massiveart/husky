            "globalize_lib",
            "ckeditor",
            "jqueryAdapter",
            "uri-template",
            "typeahead",
            "tagsManager",

            "husky",
            "__component__$navigation@husky",
            "__component__$header@husky",
            "__component__$button@husky",
            "__component__$column-options@husky",
            "__component__$datagrid@husky",
            "__component__$dialog@husky",
            "__component__$dropdown@husky",
            "__component__$matrix@husky",
            "__component__$search@husky",
            "__component__$tabs@husky",
            "__component__$toolbar@husky",
            "__component__$auto-complete@husky",
            "__component__$auto-complete-list@husky",
            "__component__$dependent-select@husky",
            "__component__$select@husky",
            "__component__$password-fields@husky",
            "__component__$column-navigation@husky",
            "__component__$loader@husky",
            "__component__$ckeditor@husky",
            "__component__$smart-content@husky",
            "__component__$overlay@husky",
            "__component__$label@husky",
            "__component__$matcher@husky",
